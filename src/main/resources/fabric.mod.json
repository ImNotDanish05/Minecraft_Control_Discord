{
  "schemaVersion": 1,
  "id": "discord-control",
  "version": "${version}",
  "name": "Discord Control",
  "description": "Control the local Minecraft client via Discord commands.",
  "authors": ["Example"],
  "contact": {
    "sources": "https://example.com"
  },
  "license": "MIT",
  "environment": "client",
  "entrypoints": {
    "client": ["com.example.discordcontrol.DiscordControlMod"]
  },
  "mixins": ["discordcontrol.mixins.json"],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "minecraft": "${minecraft_version}",
    "fabric": "*"
  }
}
